{
  "name": "skeleton-server-api",
  "version": "1.0.0",
  "description": "Skeleton for create a RESTful API server made in express, typescript e mongoose",
  "main": "app.js",
  "scripts": {
    "start": "tsc && node ./dist/app.js",
    "start:dev": "tsc && nodemon ./dist/app.js",
    "cz-init": "commitizen init cz-conventional-changelog --dev -exact"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZeroWk-EM/StarWarsAPI-Server.git"
  },
  "keywords": [
    "api",
    "mongoose",
    "typescript"
  ],
  "author": "Emanuele Moncada",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^7.0.2"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "@types/express": "^4.17.17",
    "nodemon": "^2.0.21",
    "typescript": "^5.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
